// Java program to move all the Uppercase characters to the End.

package moveuppercasetoend;
public class MoveUppercaseToEnd {
    static void uppercasetoend(String str){
        String temp="";
        for(int i=0;i<str.length();i++){
            if(str.charAt(i)>=65 && str.charAt(i)<=90)
                temp+=str.charAt(i);
            else
                System.out.print(str.charAt(i));
        }
        System.out.print(temp+"\n");
    }
    public static void main(String[] args) {
        String str="HELlo";
        uppercasetoend(str);
    }
    
}

OUTPUT:
loHEL
