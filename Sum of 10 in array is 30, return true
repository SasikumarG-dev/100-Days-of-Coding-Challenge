package sumof10is30;
public class Sumof10is30 {
    public static void main(String[] args) {
        int arr[]={10, 77, 10, 54, -11, 10,10};
        int j=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]==10)
                ++j;
        }
        if(j==3)
            System.out.println("TRUE");
        else
            System.out.println("FALSE");
    }
}

OUTPUT:
FALSE
