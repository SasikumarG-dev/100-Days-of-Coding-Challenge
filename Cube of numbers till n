package cube;
import java.util.*;
public class Cube {

    public static void main(String[] args) {
        int cube;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter number");
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
            cube=i*i*i;
            System.out.println("cube of " +i+ " is "+cube);
        }
            
    }
    
}

OUTPUT:
enter number
6
cube of 1 is 1
cube of 2 is 8
cube of 3 is 27
cube of 4 is 64
cube of 5 is 125
cube of 6 is 216
