// Java program to convert Stiring to Byte Array.

package converstringtobytearray;
public class ConverStringToByteArray {
    public static void main(String[] args) {
        String str="Welcome to my GitHub profile";
        byte arr[]=str.getBytes();
        for(int i:arr)
        System.out.println(i);
    }
}

