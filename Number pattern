package pattern;
import java.util.Scanner;
public class Pattern {
    public static void main(String[] args) {
        int i,j;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter number");
        int n=sc.nextInt();
        for(i=1;i<=n;i++){
            for(j=i;j<=n;j++){
                System.out.print(j);
            }
            System.out.println();
        }
    }   
}

OUTPUT:
1234
234
34
4
