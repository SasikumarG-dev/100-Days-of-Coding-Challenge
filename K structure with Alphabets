// Java program to print Alphabets like 'K' structure.

package kpatternwithalphabets;
public class KpatternWithAlphabets {
    public static void main(String[] args) {
        String str="ABCDEF";
        int n=str.length();
        for(int i=n-1;i>=0;i--){
            for(int j=0;j<=i;j++)
                System.out.print(str.charAt(j)+" ");
            System.out.println("");
        }
        for(int i=0;i<n;i++){
            for(int j=0;j<=i;j++)
                System.out.print(str.charAt(j)+" ");
            System.out.println("");
        }
    }
}

OUTPUT:
A B C D E F 
A B C D E 
A B C D 
A B C 
A B 
A 
A 
A B 
A B C 
A B C D 
A B C D E 
A B C D E F
