package RemoveDuplicates;
public class RemoveDuplicates {
    public static void main(String[] args) {
        int arr[]={0,0,1,1,1,2,2,3,3,4};
        int count=1;
        for(int i=0;i<arr.length-1;i++){
                if(arr[i]!=arr[i+1]){
                    arr[count++]=arr[i+1];
                }
        }
        for(int i=0;i<count;i++)
            System.out.println(arr[i]);
    }
}

OUTPUT:
0
1
2
3
4
