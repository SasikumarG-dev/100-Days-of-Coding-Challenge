package duplicatezeros;
public class DuplicateZeros {
    public static void main(String[] args) {
    int arr[] = {1,0, 2, 3, 5,  7,0, 8};
    for (int i=arr.length-1;i>=0;i--){
        if(arr[i]==0){
            for(int j=arr.length-1;j>i;j--){
                arr[j]=arr[j-1];
            }
        }
    } 
    for(int n:arr)
            System.out.println(n);
    }
}
